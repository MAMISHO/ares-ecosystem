{
  "services": [
    {
      "verbose": false,
      "respawn": false,
      "id": "webos",
      "active": true,
      "type": [
        "build",
        "test"
      ],
      "name": "webOS",
      "params": [
        "@PLUGINDIR@/ares/server/bdWebos.js",
        "-l",
        "http",
        "-I",
        "@INSTALLDIR@",
        "-P",
        "/webos",
        "-p",
        "0",
        "-t",
        "240000"
      ],
      "command": "@NODE@",
      "useJsonp": false,
      "pluginClient": "@PLUGINURL@/ares/client/package.js"
    },
    {
      "id": "genZip",
      "sources": [
        {
          "id": "bootplate-web",
          "type": "template",
          "version": "0.0.1",
          "isDefault": false,
          "description": "Basic web template for webOS - none enyo library.",
          "class": "basic",
          "deps": [
            "webappinfo",
            "webicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/bootplate-web"
            }
          ]
        },
        {
          "id": "bootplate-moonstone",
          "type": "template",
          "version": "2.5",
          "isDefault": true,
          "description": "Enyo moonstone app",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            }
          ]
        },
        {
          "id": "webos-service",
          "type": "webosService",
          "version": "0.1",
          "description": "webOS Node.js Service Template",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/webos-service",
              "prefixToAdd": ""
            }
          ],
          "filesubstitution": [
            "package.json",
            "services.json",
            "helloworld_webos_service.js",
            "helloclient.js"
          ]
        },
        {
          "id": "webappinfo",
          "type": "appinfo",
          "version": "0.0.1",
          "description": "minimal appinfo.json for webOS Application",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/webappinfo"
            }
          ],
          "filesubstitution": [
            "appinfo.json"
          ]
        },
        {
          "id": "enyoappinfo",
          "type": "appinfo",
          "version": "0.0.1",
          "description": "appinfo.json to use built-in enyo framework on WebOS Device",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/enyoappinfo"
            }
          ],
          "filesubstitution": [
            "appinfo.json"
          ]
        },
        {
          "id": "webicon",
          "type": "image",
          "version": "0.0.1",
          "description": "normal icon files [icon.png (80x80), largeIcon.png (130x130)]",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/webicon"
            }
          ]
        },
        {
          "id": "enyoicon",
          "type": "image",
          "version": "0.0.1",
          "description": "enyo icon files [icon.png (80x80), largeIcon.png (130x130)]",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/enyoicon"
            }
          ]
        },
        {
          "id": "moonstone-2.5",
          "type": "bootplate",
          "version": "2.5",
          "onDevice": true,
          "description": "bootplate-moonstone",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/empty",
              "symlink": {
                "enyo": "@PLUGINDIR@/templates/bootplate-moonstone/enyo",
                "lib": "@PLUGINDIR@/templates/bootplate-moonstone/lib"
              }
            }
          ]
        },
        {
          "id": "moonstone-2.3",
          "type": "bootplate",
          "version": "2.3",
          "onDevice": true,
          "description": "bootplate-moonstone",
          "files": [
            {
              "url": "@PLUGINDIR@/templates/empty",
              "symlink": {
                "enyo": "@PLUGINDIR@/templates/bootplate-moonstone-2.3/enyo",
                "lib": "@PLUGINDIR@/templates/bootplate-moonstone-2.3/lib"
              }
            }
          ]
        },
        {
          "id": "bootplate-moonstone-Wizard",
          "type": "template",
          "version": "2.5",
          "isDefault": false,
          "description": "Enyo moonstone bootplate Wizard",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            },
            {
              "prefixToAdd": "source",
              "url": "@PLUGINDIR@/templates/bootplate-templates/moonstone/Wizard/source"
            }
          ]
        },
        {
          "id": "bootplate-moonstone-ActivityPanelsWithVideo",
          "type": "template",
          "version": "2.5",
          "isDefault": false,
          "description": "Enyo moonstone bootplate ActivityPanels With Video",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            },
            {
              "url": "@PLUGINDIR@/templates/bootplate-templates/moonstone/ActivityPanelsWithVideo/source",
              "prefixToAdd": "source"
            }
          ]
        },
        {
          "id": "bootplate-moonstone-AlwaysViewingPanelsWithVideo",
          "type": "template",
          "version": "2.5",
          "isDefault": false,
          "description": "Enyo moonstone booplate AlwaysViewingPanels With video",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            },
            {
              "prefixToAdd": "source",
              "url": "@PLUGINDIR@/templates/bootplate-templates/moonstone/AlwaysViewingPanelsWithVideo/source"
            }
          ]
        },
        {
          "id": "bootplate-moonstone-AlwaysViewingPanels",
          "type": "template",
          "version": "2.5",
          "isDefault": false,
          "description": "Enyo moonstone booplate AlwaysViewingPanels",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            },
            {
              "url": "@PLUGINDIR@/templates/bootplate-templates/moonstone/AlwaysViewingPanels/source",
              "prefixToAdd": "source"
            }
          ]
        },
        {
          "id": "bootplate-moonstone-ActivityPanels",
          "type": "template",
          "version": "2.5",
          "isDefault": false,
          "description": "Enyo moonstone bootplate ActivityPanels",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            },
            {
              "prefixToAdd": "source",
              "url": "@PLUGINDIR@/templates/bootplate-templates/moonstone/ActivityPanels/source"
            }
          ]
        },
        {
          "id": "bootplate-moonstone-MediaBrowser",
          "type": "template",
          "version": "2.5",
          "isDefault": false,
          "description": "Enyo moonstone bootplate MediaBrowser",
          "class": "moonstone",
          "deps": [
            "enyoappinfo",
            "enyoicon"
          ],
          "files": [
            {
              "url": "@PLUGINDIR@/templates/app-moonstone"
            },
            {
              "url": "@PLUGINDIR@/templates/bootplate-templates/moonstone/MediaBrowser/source",
              "prefixToAdd": "source"
            }
          ]
        }
      ]
    }
  ]
}
